<div class="page-header">
    <h1>New model</h1>
</div>

<% form_for(model, {action: path_to.models, method: 'POST', id: "model_form", class: 'form-horizontal'}, function (form) { %>
    <%- partial('models/form.ejs', {locals: {form: form, model: model}}) %>
    <div class="form-actions">
        <%- form.submit('<i class="icon-ok icon-white"></i> Create model', {class: 'btn btn-primary'}) %> or
        <%- link_to('Cancel', path_to.models, {class: 'btn'}) %>
    </div>
<% });%>
